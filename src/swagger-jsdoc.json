{
    "swagger": "2.0",
    "info": {
        "title": "SQL Server Registration Swagger NodeJS API",
        "version": "1.0.0",
        "description": "Endpoints to test the user registration routes"
    },
    "host": "localhost:8000",
    "basePath": "/",
    "securityDefinitions": {
        "bearerAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "scheme": "bearer",
        "in": "header"
        }
    }
}